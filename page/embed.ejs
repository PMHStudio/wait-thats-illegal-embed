<meta http-equiv="refresh" content="0; url=/create">
<% if (embed.success) { %>
<meta name="theme-color" content="<%= embed.data.color %>">
<meta name="twitter:title" content="<%= embed.data.top %>">
<meta property="og:title" content="<%= embed.data.middle %>">
<meta property="og:description"content="<%= embed.data.bottom %>">
<link rel="shortcut icon" href="<%= embed.data.image %>" type="image/x-icon">
<% } else { %>
<meta name="theme-color" content="#FF0000">
<meta name="twitter:title" content="Tritium Networks, Embed Maker">
<meta property="og:title" content="임배드를 찾을 수 없습니다">
<meta property="og:description"content="Url을 다시한번 확인해보세요">
<link rel="shortcut icon" href="https://image.freepik.com/free-vector/x-letter_53876-60368.jpg" type="image/x-icon">
<% } %>
