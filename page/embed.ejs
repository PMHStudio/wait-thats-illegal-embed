<meta http-equiv="refresh" content="0; url=/create">
<% if (embed.success) { %>
<meta name="theme-color" content="<%= embed.data.color %>">

<meta name="twitter:card" content="<%= embed.data.top %>">
<meta name="application-name" content="<%= embed.data.top %>">

<meta property="og:title" content="<%= embed.data.middle %>">
<meta property="og:description"content="<%= embed.data.bottom %>">

<meta property="og:image" content="<%= embed.data.image %>">
<meta property="twitter:image:src" content="<%= embed.data.image %>">
<link rel="shortcut icon" href="<%= embed.data.image %>" type="image/x-icon">

<% } else { %>
<meta name="theme-color" content="#FF0000">

<meta name="twitter:card" content="Tritium Networks">
<meta name="application-name" content="Tritium Networks">

<meta property="og:title" content="임배드를 찾을 수 없습니다">
<meta property="og:description"content="Url을 다시한번 확인해보세요">

<meta property="og:image" content="https://image.freepik.com/free-vector/x-letter_53876-60368.jpg">
<meta property="twitter:image:src" content="https://image.freepik.com/free-vector/x-letter_53876-60368.jpg">
<link rel="shortcut icon" href="https://image.freepik.com/free-vector/x-letter_53876-60368.jpg" type="image/x-icon">

<% } %>
